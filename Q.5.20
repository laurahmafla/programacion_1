% part(b),  plots of bY3c,Y6f
X = linspace(-2,2,36);
subplot (2,2,2);
Y31 = -5*X.^2;
Y32 = -2*X.^2;
Y33 = -X.^2;
Y34 = -.5*X.^2;
Y35 = -.1*X.^2;
plot (X,Y31,X,Y32,X,Y33,X,Y34,X,Y35);xlabel('x');
title ('Y3c = c*x^2, for c = -5,-2,-1,-.5,-.1');
ylabel('Amplitude of Y3c');
X = linspace(-2,2,36);
subplot(2,1,1);
Y61=5*X.^2;
Y62 = 2*X.^2;
Y63 = X.^2;
Y64 =.5*X.^2;
Y65 =.1*X.^2;
plot(X,Y61,X,Y62,X,Y63,X,Y64,X,Y65);
ylabel('Amplitude of Y6f');  xlabel('x')
title('Y6f = f*x^2, for f = 5,2,1,0.5,0.1')
