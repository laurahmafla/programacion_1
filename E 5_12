x = linspace(1,12);
y1 = sin(x);
y2 = sin(3*x);
y3 = sin(5*x);
y4 = sin(9*x);

figure
subplot(1,2,1)
plot(x,y1)
title('Subplot 1: sin(x)')

subplot(1,2,2)
plot(x,y2)
title('Subplot 2: sin(2x)')

subplot(1,2,3)
plot(x,y3)
title('Subplot 3: sin(4x)')

subplot(1,2,4)
plot(x,y4)
title('Subplot 4: sin(8x)')
