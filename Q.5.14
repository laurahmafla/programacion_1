 % part(b),  plots of Y2b, Y3c, Y4d, and Y5e
 X = linspace(-2,2,36);
 subplot (2,2,1);
 Y21 = 5*X.^2;
 Y22 = 2*X.^2;
 Y23 = X.^2;
 Y24 = -5*X.^2;
 Y25 = -2*X.^2;
 plot (X,Y21,X,Y22,X,Y23,X,Y24,X,Y25)
