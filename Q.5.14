 subplot (2,2,2);
 Y31 = -5*X.^2;
 Y32 = -2*X.^2;
 Y33 = -X.^2;
 Y34 = -.5*X.^2;
 Y35 = -.1*X.^2;
 plot (X,Y31,X,Y32,X,Y33,X,Y34,X,Y35);xlabel('x');
 title ('Y3c = c*x^2, for c = -5,-2,-1,-.5,-.1');
