% part(b),  plots of Y2b, bY3c
X = linspace(-2,2,36);
subplot (2,2,1);
Y21 = 5*X.^2;
Y22 = 2*X.^2;
Y23 = X.^2;
Y24 = -5*X.^2;
Y25 = -2*X.^2;
plot (X,Y21,X,Y22,X,Y23,X,Y24,X,Y25)
title ('Y2b = b*x^2, for b = 5,2,1,-5,-2');
ylabel('Amplitude of Y2b');xlabel('x');
subplot (2,2,2);
Y31 = -5*X.^2;
Y32 = -2*X.^2;
Y33 = -X.^2;
Y34 = -.5*X.^2;
Y35 = -.1*X.^2;
plot (X,Y31,X,Y32,X,Y33,X,Y34,X,Y35);xlabel('x');
title ('Y3c = c*x^2, for c = -5,-2,-1,-.5,-.1');
ylabel('Amplitude of Y3c');
